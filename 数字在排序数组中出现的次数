/*
统计一个数字在排序数组中出现的次数
*/

//遍历一次
public class Solution {
    public int GetNumberOfK(int [] array , int k) {
        int count = 0;
        for (int i = 0; i < array.length; i++) {
            if (array[i] == k) {
                count++;
            } else if (array[i] > k) {
                break;
            }
        }
        return count;
    }
}
